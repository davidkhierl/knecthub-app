{
  "typescript.tsdk": ".yarn/sdks/typescript/lib",
  "conventionalCommits.scopes": [
    "package",
    "Container",
    "ProfileCard",
    "useUserStore",
    "MainLayout",
    "AccountPage",
    "RegisterForm",
    "UpdateUserForm",
    "EmailVerify"
  ],
  "search.exclude": {
    "**/.yarn": true,
    "**/.pnp.*": true
